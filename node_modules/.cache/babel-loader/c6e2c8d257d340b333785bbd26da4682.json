,CAAC,IADI;AAEf,kBAAA,EAAE,EAAE;AAFW,iBAAjB;;AAID,gBAAA,OAAO,CAAC,GAAR,CAAY,IAAZ;AACA,gBAAA,KAAI,CAAC,IAAL,GAAY,EAAZ;;AAzBc;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AA0Bf,KA3BM;AA4BD,IAAA,UA5BC,wBA4BY;AAAA;;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAEf,gBAAA,MAAI,CAAC,OAAL,GAAe,IAAf;AAFe;AAAA,uBAGM,KAAK,CAAC,GAAN,CAAU,wCAAV,CAHN;;AAAA;AAAA;AAGR,gBAAA,IAHQ,oBAGR,IAHQ;;AAAA,oBAIV,IAJU;AAAA;AAAA;AAAA;;AAAA,sBAKP,IAAI,KAAJ,CAAU,mBAAV,CALO;;AAAA;AAOf,gBAAA,UAAU,CAAC,YAAM;AACf,kBAAA,MAAI,CAAC,MAAL,GAAc,IAAd;AAEA,kBAAA,MAAI,CAAC,OAAL,GAAe,KAAf;AACD,iBAJS,EAIP,CAJO,CAAV;AAPe;AAAA;;AAAA;AAAA;AAAA;AAcf,gBAAA,MAAI,CAAC,OAAL,GAAe,KAAf;AACA,gBAAA,MAAI,CAAC,KAAL,GAAa;AACX,kBAAA,IAAI,EAAE,QADK;AAEX,kBAAA,KAAK,EAAE,QAFI;AAGX,kBAAA,IAAI,EAAE,aAAE;AAHG,iBAAb;AAKA,gBAAA,OAAO,CAAC,GAAR,CAAY,aAAE,OAAd;;AApBe;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkClB,KA9DM;AA+DD,IAAA,YA/DC,wBA+DY,EA/DZ,EA+DgB;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEb,gBAAA,MAFa,GAEJ,MAAI,CAAC,MAAL,CAAY,IAAZ,CAAiB,UAAA,MAAK;AAAA,yBAAK,MAAM,CAAC,EAAP,KAAc,EAAnB;AAAA,iBAAtB,EAA6C,IAFzC;AAAA;AAAA,uBAGb,KAAK,UAAL,iDAAsD,EAAtD,EAHa;;AAAA;AAInB,gBAAA,MAAI,CAAC,MAAL,GAAe,MAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,UAAA,MAAK;AAAA,yBAAK,MAAM,CAAC,EAAP,KAAc,EAAnB;AAAA,iBAAxB,CAAf;AACA,gBAAA,MAAI,CAAC,KAAL,GAAa;AACX,kBAAA,IAAI,EAAE,SADK;AAEX,kBAAA,KAAK,EAAE,UAFI;AAGX,kBAAA,IAAI,mIAA4B,MAA5B;AAHO,iBAAb;AALmB;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcvB;AA7EO,GAZI;AA2Fb,EAAA,UAAU,EAAE;AAAC,IAAA,aAAa,EAAb,aAAD;AAAgB,IAAA,QAAQ,EAAR,QAAhB;AAA0B,IAAA,SAAS,EAAT;AAA1B;AA3FC,CAAf","sourcesContent":["<template>\r\n\r\n  <div class=\"container\">\r\n\r\n    <app-alert :alert=\"alert\" @close=\"alert = null\"></app-alert>\r\n\r\n    <form @submit.prevent=\"changes\">\r\n    <div class=\"card\">\r\n      <h2>Работа с базой данных</h2>\r\n      <div class=\"form-control\">\r\n      <label for=\"name\">ВЫАЫВ</label>\r\n      <input type=\"text\" id=\"name\" v-model.trim=\"name\">\r\n      </div>\r\n      <button class=\"btn primary\" @click=\"loadPeople\">werwer</button>\r\n    </div>\r\n    </form>\r\n\r\n    <app-loader v-if=\"loading\"></app-loader>\r\n\r\n    <app-people-list\r\n        v-else\r\n        :people=\"people\"\r\n        @load=\"loadPeople\"\r\n        @remove=\"removePerson\"\r\n    ></app-people-list>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\n\r\nimport AppPeopleList from \"@/AppPeopleList\";\r\nimport axios from 'axios'\r\nimport AppAlert from \"./AppAlert\";\r\nimport AppLoader from \"./AppLoader\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      name: '',\r\n      people: [],\r\n      alert: null,\r\n      loading: false\r\n    }\r\n  },\r\n  mounted() {\r\n    this.loadPeople()\r\n  },\r\n  methods: {\r\n    async changes() {\r\n      // const xhr = new XMLHttpRequest()\r\n      // xhr.open('GET', 'http://itmomafia.herokuapp.com/player/')\r\n      //\r\n      // xhr.onload = () => {\r\n      //   console.log(xhr.response)\r\n      // }\r\n      // xhr.send()\r\n\r\n      const response = await fetch(\r\n            'http://itmomafia.herokuapp.com/player/',{\r\n            method: 'POST',\r\n            headers: {\r\n              'Content-Type': 'application/json'\r\n            },\r\n            body: JSON.stringify({\r\n              name: this.name\r\n            })\r\n          })\r\n      const test = await response.json()\r\n       this.people.push({\r\n         name: this.name,\r\n         id: test\r\n       })\r\n      console.log(test)\r\n      this.name = ''\r\n    },\r\n    async loadPeople() {\r\n      try {\r\n        this.loading = true;\r\n        const {data} = await axios.get('http://itmomafia.herokuapp.com/player/')\r\n        if (!data) {\r\n          throw new Error('Список людей пуст')\r\n        }\r\n        setTimeout(() => {\r\n          this.people = data\r\n\r\n          this.loading = false;\r\n        }, 1)\r\n\r\n      } catch (e) {\r\n        this.loading = false;\r\n        this.alert = {\r\n          type: 'danger',\r\n          title: 'Ошибка',\r\n          text: e.message\r\n        }\r\n        console.log(e.message);\r\n      }\r\n      //const {data} = await axios.get('http://itmomafia.herokuapp.com/player/')\r\n      // const result = Object.keys(data).map(\r\n      //   key => {\r\n      //     return {\r\n      //       id: data[id],\r\n      //       name: data[name]\r\n      //     }\r\n      //   }\r\n      // )\r\n\r\n\r\n\r\n    },\r\n    async removePerson(id) {\r\n      try {\r\n        const person = this.people.find(person => person.id === id).name\r\n        await axios.delete(`http://itmomafia.herokuapp.com/player/${id}`)\r\n        this.people =  this.people.filter(person => person.id !== id)\r\n        this.alert = {\r\n          type: 'primary',\r\n          title: 'Успешно!',\r\n          text: `Пользователь с именем \"${person}\" был удален`\r\n        }\r\n      }catch (e) {\r\n\r\n      }\r\n\r\n    }\r\n  },\r\n  components: {AppPeopleList, AppAlert, AppLoader}\r\n}\r\n</script>\r\n\r\n<style>\r\n\r\n</style>\r\n"],"sourceRoot":""}]}